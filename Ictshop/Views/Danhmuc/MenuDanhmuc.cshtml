@using Ictshop.Models;
@model List<Hangsanxuat>

<div class="row">
    @if (Model != null)
    {
        // Sử dụng Model.Property ở đây
        <div id="myDropdown" class="dropdown-menu">
            @foreach (var item in Model)
            {
                <a class="dropdown-item" href="@Url.Action("dttheohang", "Sanpham", new { brandId = item.Mahang })">@item.Tenhang</a>

            }
            
            </div>
    }
        </div>

